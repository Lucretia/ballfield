with "SDLAda";

project Ballfield is
  for Source_Dirs use ("source/");
  for Object_Dir  use "object/";
  for Exec_Dir    use "./";
  for Create_Missing_Dirs use "True";
  for Main use ("main.adb");

  package Build is
     for Default_Switches use ("-gnaty", "-gnatwa");
  end Build;

end Ballfield;
